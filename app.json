{
  "expo": {
    "name": "willfind8",
    "slug": "willfind8",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "willfind8",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.willfind8"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "backgroundColor": "#00000000",
      "package": "com.willfind8"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-localization",
      "expo-secure-store",
      [
        "react-native-fbsdk-next",
        {
          "appID": "1481502439945416",
          "clientToken": "fb5c688f8faf7df7ddd9774ef2a463ae",
          "displayName": "Willfind8",
          "scheme": "fb1481502439945416",
          "advertiserIDCollectionEnabled": false,
          "autoLogAppEventsEnabled": false,
          "isAutoInitEnabled": true,
          "iosUserTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.181055438274-243ie9q3uj0f8igug6h73pusnmrg0t6c"
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "$(PRODUCT_NAME) would like to use your camera to share image in posts and messages.",
          "photosPermission": "$(PRODUCT_NAME) would like to use your device gallery to attach image in a posts and messages."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "50e78cd9-eacc-48f2-9c70-e04eb0009a0a"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/8d16a046-45ac-43e4-b730-33c7a0d6d1ea"
    },
    "owner": "devcodewrite"
  }
}
